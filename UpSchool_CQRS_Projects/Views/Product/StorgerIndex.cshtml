@model List<GetProductStoragerQueryResult>
@{
    ViewData["Title"] = "StorgerIndex";
}




<div class="container">

    <h1>Depocu Paneli</h1>
    <table class="table table-bordered">
        <tr>
            <th>#</th>
            <th>Ürün Adı</th>
            <th>Deposu </th>
            <th>Sil </th>
            <th>Güncelle </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ProductID</td>
                <td>@item.Name</td>
                <td>@item.Storage</td>
                <td><a href="#" class="btn btn-outline-danger">Sil</a></td>
                <td><a href="#" class="btn btn-outline-warning">Güncelle</a></td>

            </tr>
        }
    </table>
    <a href="#" class="btn btn-outline-primary">Yeni Kayıt Girişi</a>
</div>

